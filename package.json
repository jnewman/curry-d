            "safari": [6],
            "opera": [12, "next"]
        },
        "harness" : "mocha",
        "files": [
            "test/curry-d-node.js"
        ]
    },
    "devDependencies": {
        "chai": "~1.7",
        "curry": "~1.0",
        "grunt": ">=0.4",
        "grunt-benchmark": "~0.2",
        "grunt-cli": "~0.1",
        "grunt-contrib-concat": "~0.3",
        "grunt-contrib-connect": "~0.3",
        "grunt-contrib-jshint": "~0.6",
        "grunt-contrib-uglify": "~0.2",
        "grunt-mocha-phantomjs": "~0.3",
        "grunt-mocha-test": "~0.5",
        "jshint": "~2.1",
        "lodash": "~1.3",
        "mocha": "~1.12",
        "requirejs": "~2.1"
    },
    "licenses": [
        {
