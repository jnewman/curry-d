{
    "browser": true,
    "camelcase": true,
    "curly": true,
    "devel": true,
    "eqeqeq": true,
    "globals": {
        "define": false,
        "require": false,
        "after": false,
        "afterEach": false,
        "before": false,
        "beforeEach": false,
        "describe": false,
        "jasmine": false,
        "it": false,
        "expect": false,
        "demand": false
    },
    "indent": 4,
    "latedef": true,
    "maxcomplexity": 10,
    "maxdepth": 6,
    "maxlen": 120,
    "maxstatements": 15,
    "node": true,
    "noempty": true,
    "newcap": true,
    "nonew": true,
    "trailing": false,
    "undef": true,
    "unused": "vars"
}
